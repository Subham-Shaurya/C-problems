#include <stdio.h>

int main(){
	int a, b, large;
		
	printf("Enter value for a:"); /*Asking for 1st integer*/
	scanf("%d", &a);
	
	printf("Enter value for b: "); /*Asking for 2nd integer*/
	scanf("%d", &b);
	/*Here using if else statement check which input is largest*/
	if(a>b){
		large=a;
	}
	else{
	    large=b;
	}
	printf("Largest number: %d",large); /* Print largest integer as output*/
    return 0;
}
